		<data name="rotation" type="coord3d" default="0,0,0" />
		<ped model="143" rotation="!rotation!"/>
		<object model="349" posZ="1" rotation="!rotation!"/>
		<data name="BotType" type="selection:ScavGuard,Civ,ScavVendor,Freelance,WasteGuard,WasteCiv,WasteVendor,WasteFreelance,Nang,Triad,SyndVendor,SyndCiv,ScavQuest,WasteQuest,SyndQuest,Pig" description="Bot role" required="true" default="Guard" />
		<data name="name" type="string" description="Bot name (optional)" required="false" default="" />
		<data name="botSkinID" type="integer" description="Bot skin ID (optional)" required="false" default="" />
