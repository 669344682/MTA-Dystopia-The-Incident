<def name="dystopia_map">

	<element name="Zombie_spawn" friendlyname="Zombie Spawnpoint" icon="edf/zombiespawn.png">
		<data name="position" type="coord3d" default="0,0,0" />
		<data name="rotation" type="coord3d" default="0,0,0" />
		<ped model="209" rotation="!rotation!"/>
		<!-- <object model="1254" posZ="1" rotation="!rotation!"/> -->
		<data name="name" type="string" description="Bot name (optional)" required="false" default="" />
		<data name="ZedType" type="selection:Walker,Runner,Brute,Special" description="Zed type" required="true" default="Walker" />
		<data name="ZedSkinID" type="integer" description="Zed skin ID (optional)" required="false" default="" />
		<data name="botRange" type="integer" description="Bot spawning range" required="true" default="100" />
	</element>
	
